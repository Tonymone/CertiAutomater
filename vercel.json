{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
